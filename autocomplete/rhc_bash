        if [[ "$cur" == -* ]]; then
          opts="--app --gears --namespace --state"
        else
          opts=""
        fi
        ;;

      "rhc app-snapshot")
        if [[ "$cur" == -* ]]; then
          opts=""
        else
          opts=""
        fi
        ;;

      "rhc app-ssh")
        if [[ "$cur" == -* ]]; then
          opts="--app --namespace --ssh"
        else
          opts=""
        fi
