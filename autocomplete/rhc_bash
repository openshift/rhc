        if [[ "$cur" == -* ]]; then
          opts="--expires-in --note --scopes"
        else
          opts=""
        fi
        ;;

      "rhc authorization delete")
        if [[ "$cur" == -* ]]; then
          opts="--auth-token"
        else
          opts=""
